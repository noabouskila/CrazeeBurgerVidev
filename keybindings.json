[
    {
        "key": "alt+m",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "background: ${1};"
        }
    },
    {
        "key": "alt+l",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "border: 1px solid ${1};"
        }
    }
]